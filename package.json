{
  "name": "ts-collections",
  "version": "1.0.0",
  "description": "List and Dictionary for Typescript",
  "main": "src/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "ts-node node_modules/tape/bin/tape tests/**/*.test.ts",
    "test:watch": "ts-node node_modules/tape-watch/bin/tape-watch tests/**/*.test.ts",
    "build": "tsc",
    "docs": "typedoc --out ./docs ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emobe/ts-boilerplate.git"
  },
  "keywords": [
    "typescript",
    "tape",
    "boilerplate",
    "testing"
  ],
  "author": "Anthony Shuker <anthony@emobe.uk>",
  "license": "UNLICENCED",
  "bugs": {
    "url": "https://github.com/emobe/ts-boilerplate/issues"
  },
  "homepage": "https://github.com/emobe/ts-boilerplate#readme",
  "devDependencies": {
    "@types/node": "^13.13.0",
    "@types/tape": "^4.13.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.6",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "tape": "^4.13.2",
    "ts-node": "^8.8.2",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "tsconfig-paths": "^3.9.0"
  }
}
